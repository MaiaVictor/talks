@Leaf = (a ((a b) (* b)))
@Node = (a (b (* ((a (b c)) c))))

@gen = (?<(a @genS) b> b)
  & @Leaf ~ (#1 a)

@genS = ({2 a b} c)
  & @Node ~ (e (d c))
  & @gen ~ (a d)
  & @gen ~ (b e)

@sum = ((@sumZ (@sumS a)) a)

@sumZ = (a a)
@sumS = (a (b c))
  & @sum ~ (a <+ e c>)
  & @sum ~ (b e)

@main = a
  & @sum ~ (b a)
  & @gen ~ (#24 b)
